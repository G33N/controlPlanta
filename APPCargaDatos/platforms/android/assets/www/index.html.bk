        }
    </script>
    <script type="text/javascript">
        $('#stockPlanta').click(function() {
            $('#sulfatoContainer').hide();
            $('#sodioContainer').hide();
            $('#salidaPlantaContainer').hide();
            $('#salidaQuimicosContainer').hide();
            $('#stockPlantaContainer').show();
            $('#resultado').html("");
        });

        function enviarStockForm() {
            $('#stockPlantaForm').submit(function() {
                $.ajax({
                    type: 'POST',
                    url: "http://aguasrionegrinas.enlaceit.com.ar/amePlanta.php",
                    data: $(this).serialize() + "&orden=" + 22,
