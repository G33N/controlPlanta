    <div id="salidaPlantaContainer" class="container action-container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 position-bottom text-center clear">
                <form id="salidaPlantaForm" class="form-horizontal">
                    <h4>SALIDA DE PLANTA</h4>
                    <div class="form-group row">
                        <label for="example-text-input" class="col-xs-4 col-form-label">Caudal</label>
                        <div class="col-xs-4">
                            <input class="form-control" type="number" value="" id="caudalSalida" name="caudalSalida">
                        </div>
                        <label for="example-text-input" class="col-xs-4 col-form-label">M<sup>3</sup>/H</label>
                    </div>
                    <div class="form-group row">
                        <label for="example-text-input" class="col-xs-4 col-form-label">Turbiedad de Entrada</label>
                        <div class="col-xs-4">
                            <input class="form-control" type="number" value="" id="turbiedadEntrada" name="turbiedadEntrada">
                        </div>
                        <label for="example-text-input" class="col-xs-4 col-form-label">NTU</label>
                    </div>
                    <div class="form-group row">
                        <label for="example-text-input" class="col-xs-4 col-form-label">Turbiedad de Salida</label>
                        <div class="col-xs-4">
                            <input class="form-control" type="number" value="" id="turbiedadSalida" name="turbiedadSalida">
                        </div>
                        <label for="example-text-input" class="col-xs-4 col-form-label">NTU</label>
                    </div>
                    <div class="form-group row">
                        <label for="example-text-input" class="col-xs-4 col-form-label">Cloro Residual</label>
