                    <div class="form-group row">
                        <label for="example-text-input" class="col-xs-4 col-form-label">Color Test</label>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="positivo" value="1" name="colorTest"> Positivo
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="negativo" value="0" name="colorTest"> Negativo
                        </label>
                    </div>
                    <div class="form-group row">
                        <div class="col-xs-12">
                            <button class="btn btn-md btn-success" type="submit" id="enviarSalidaPlanta">ENVIAR</button>
                        </div>
                    </div>
