    <script type="text/javascript">
        $('#salidaPlanta').click(function() {
            $('#sulfatoContainer').hide();
            $('#sodioContainer').hide();
            $('#salidaPlantaContainer').show();
            $('#salidaQuimicosContainer').hide();
            $('#stockPlantaContainer').hide();
            $('#resultado').html("");
        });

        function enviarSalidaPlanta() {
            $('#salidaPlantaForm').submit(function() {
                $.ajax({
                    type: 'POST',
                    url: "http://aguasrionegrinas.enlaceit.com.ar/amePlanta.php",
                    data: $(this).serialize() + "&orden=" + 21,
                    success: function(data) {
                        if (data.success === true) {
                            alert(data.message);
                        }
                        if (data.success === false) {
                            alert(data.message);
                        }
                    }
                });
                return false;
            });
        }
    </script>
    <script type="text/javascript">
        $('#salidaQuimicos').click(function() {
